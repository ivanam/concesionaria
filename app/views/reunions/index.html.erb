<p id="notice"><%= notice %></p>

<h1>Reunions</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Semana</th>
      <th>Lugar fisico</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reunions.each do |reunion| %>
      <tr>
        <td><%= reunion.fecha %></td>
        <td><%= reunion.semana %></td>
        <td><%= reunion.lugar_fisico %></td>
        <td><%= reunion.user_id %></td>
        <td><%= link_to 'Show', reunion %></td>
        <td><%= link_to 'Edit', edit_reunion_path(reunion) %></td>
        <td><%= link_to 'Destroy', reunion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reunion', new_reunion_path %>
