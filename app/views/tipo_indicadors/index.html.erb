<p id="notice"><%= notice %></p>

<h1>Tipo Indicadors</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_indicadors.each do |tipo_indicador| %>
      <tr>
        <td><%= tipo_indicador.descripcion %></td>
        <td><%= link_to 'Show', tipo_indicador %></td>
        <td><%= link_to 'Edit', edit_tipo_indicador_path(tipo_indicador) %></td>
        <td><%= link_to 'Destroy', tipo_indicador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Indicador', new_tipo_indicador_path %>
