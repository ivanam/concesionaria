<h1>Tipo Objetivos</h1>

<p id="notice"><%= notice %></p>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Descripcion</th>
        <th class="col-md-2">&nbsp;</th>
      </tr>
    </thead>

    <tbody>
      <% @tipo_objetivos.each do |tipo_objetivo| %>
        <tr>
        <td><%= tipo_objetivo.descripcion %></td>
          <td class="text-center">
            <%= link_to tipo_objetivo, class: 'btn btn-sm btn-default' do %>
              <span class="glyphicon glyphicon-search"></span>
            <% end %>
            <%= link_to edit_tipo_objetivo_path(tipo_objetivo), class: 'btn btn-sm btn-default' do %>
              <span class="glyphicon glyphicon-pencil"></span>
            <% end %>
            <%= link_to tipo_objetivo, class: 'btn btn-sm btn-danger',method: :delete, data: { confirm: '¿Está seguro?' } do %>
              <span class="glyphicon glyphicon-trash"></span>
            <% end %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br>
<div class="page-header">
  <%= link_to new_tipo_objetivo_path, class: 'btn btn-primary' do %>
    <%= fa_icon "plus", text: "Nuevo tipo de Indicador" %>
  <% end %>
</div>
