<p id="notice"><%= notice %></p>

<h1>Concesionaria</h1>

<table>
  <thead>
    <tr>
      <th>Icono</th>
      <th>Nombre</th>
      <th>Fecha alta</th>
      <th>Fecha baja</th>
      <th>User</th>
      <th>Empresa</th>
      <th>Persona</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concesionaria.each do |concesionarium| %>
      <tr>
        <td><%= concesionarium.icono %></td>
        <td><%= concesionarium.nombre %></td>
        <td><%= concesionarium.fecha_alta %></td>
        <td><%= concesionarium.fecha_baja %></td>
        <td><%= concesionarium.user_id %></td>
        <td><%= concesionarium.empresa_id %></td>
        <td><%= concesionarium.persona_id %></td>
        <td><%= link_to 'Show', concesionarium %></td>
        <td><%= link_to 'Edit', edit_concesionarium_path(concesionarium) %></td>
        <td><%= link_to 'Destroy', concesionarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concesionarium', new_concesionarium_path %>
