<div class="card card-container">
    <!-- <img class="profile-img-card" src="//lh3.googleusercontent.com/-6V8xOA6M7BA/AAAAAAAAAAI/AAAAAAAAAAA/rzlHcD0KYwo/photo.jpg?sz=120" alt="" /> -->
    <img id="profile-img" class="profile-img-card" src="//ssl.gstatic.com/accounts/ui/avatar_2x.png" />
    <p id="profile-name" class="profile-name-card"></p>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => {:class => "form-signin"}) do |f| %>
        <span id="reauth-email" class="reauth-email"></span>
        <%= f.email_field :email, autofocus: true, placeholder: "Dirección de Email", :class => "form-control", :type => "email", :id => "inputEmail"  %>
        
        <%= f.password_field :password, autocomplete: "off", :class => "form-control", :id => "inputPassword", :placeholder => "Contraseña"  %>
        <%= f.submit "Entrar", :class => "btn btn-lg btn-primary btn-block btn-signin" %>
    <% end %>
 
</div>





